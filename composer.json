{
    "name": "pixelpoems/silverstripe-selection-field",
    "type": "silverstripe-vendormodule",
    "description": "Silverstripe Module that provides a selection field for the CMS.",
    "keywords": [
        "silverstripe",
        "field"
    ],
    "authors": [
        {
            "name": "Bettina BrÃ¶thaler",
            "homepage": "https://www.pixelpoems.com/",
            "role": "Lead Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "silverstripe/cms": ">=4",
        "silverstripe/framework": ">=4",
        "silverstripe/vendor-plugin": ">=1",
        "heyday/silverstripe-colorpalette": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Pixelpoems\\SelectionField\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "extra": {
        "installer-name": "silverstripe-selection-field",
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "expose": [
            "client/dist/css",
            "client/dist/fonts"
        ]
    },
    "license": "BSD-3-Clause"
}
