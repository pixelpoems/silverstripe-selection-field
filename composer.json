{
    "name": "pixelpoems/silverstripe-selection-field",
    "type": "silverstripe-vendormodule",
    "description": "Silverstripe Module that provides a selection field for the CMS.",
    "keywords": [
        "silverstripe",
        "field"
    ],
    "authors": [
        {
            "name": "Bettina Wagner-LÃ¶ffler",
            "homepage": "https://www.pixelpoems.com/",
            "role": "Lead Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/silverstripeltd/silverstripe-colorpalette"
        }
    ],
    "require": {
        "php": "^8.3",
        "silverstripe/cms": "^6.0",
        "silverstripe/framework": "^6.0",
        "heyday/silverstripe-colorpalette": "dev-pulls/cms6-support"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Pixelpoems\\SelectionField\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "extra": {
        "installer-name": "silverstripe-selection-field",
        "expose": [
            "client/dist"
        ]
    },
    "license": "BSD-3-Clause"
}
